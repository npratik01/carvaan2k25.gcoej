    .hero-baap .hero-video {
      width: 100vw;
      height: 100vh;
      object-fit: cover;
      position: absolute;
      top: 0;
      left: 0;
      transform: none;
    }
  }

  /* Custom CSS for tech-themed cards with blue variations */
  .event-card {
    background: linear-gradient(145deg, #1a1a1a, #0d0d0d);
    border: 1px solid #333;
    border-radius: 12px;
    box-shadow: 0 4px 15px rgba(0, 123, 255, 0.2);
    /* Blue shadow */
    transition: transform 0.3s ease, box-shadow 0.3s ease;
    margin: 20px 30px;
  }

  .event-card:hover {
    transform: translateY(-5px);
    box-shadow: 0 8px 25px rgba(0, 123, 255, 0.4);
    /* Blue shadow */
  }

  .event_name {
    font-family: "Big Shoulders Stencil", sans-serif;
    font-size: 30px;
  }

  .event-title {
    color: #007bff;
    /* Bright blue */
    font-size: 1.5rem;
    font-weight: bold;
    text-shadow: 0 0 5px rgba(0, 123, 255, 0.7);
    /* Blue glow */
  }

  .event-card p {
    color: #b3b3b3;
    /* Light gray for text */
  }

  .btn {
    background: linear-gradient(145deg, #007bff, #0056b3);
    /* Blue gradient */
    color: #fff;
    /* White text for better contrast */
    border: none;
    border-radius: 8px;
    padding: 8px 16px;
    font-weight: bold;
    cursor: pointer;
    transition: background 0.3s ease, transform 0.3s ease;
  }

  .btn:hover {
    background: linear-gradient(145deg,
        #0056b3,
        #007bff);
    /* Reverse blue gradient */
    transform: scale(1.05);
  }

  .btn-secondary {
    background: transparent;
    border: 2px solid #007bff;
    /* Blue border */
    color: #007bff;
    /* Blue text */
  }

  .btn-secondary:hover {
    background: rgba(0, 123, 255, 0.1);
    /* Light blue background on hover */
  }

  body {
    font-family: "Roboto Condensed", serif;
  }

  .hero {
    background-color: #000;
    margin: auto;
  }

  .heroBaap {
    background-color: #000;
    margin: auto;
  }

  h1,
  h2 {
    color: white;
    font-weight: 400px;
    text-align: center;
    font-size: large;
  }

  /* .sponsors{
      justify-content: center;
      justify-items: center;
    } */

  .prize-card {
    width: 570px;
    height: 300px;
  }

  @keyframes neonGlow {
    0% {
      box-shadow: 0 0 10px rgb(0, 183, 255);
    }

    100% {
      box-shadow: 0 0 20px rgb(0, 183, 255);
    }
  }

  .animate-glow {
    animation: neonGlow 2s infinite alternate;
  }

  @keyframes floating {

    0%,
    100% {
      transform: translateY(0);
    }

    50% {
      transform: translateY(-10px);
    }
  }

  .animate-floating {
    animation: floating 2s infinite ease-in-out;
  }
</style>

